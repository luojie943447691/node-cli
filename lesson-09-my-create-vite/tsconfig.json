{
  "compilerOptions": {
    "outDir": "dist",
    "types": [
      "node"
    ],
    "target": "es2016",
    "module": "Node16",
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
  },
  "exclude": [
    "template-**"
  ]
}